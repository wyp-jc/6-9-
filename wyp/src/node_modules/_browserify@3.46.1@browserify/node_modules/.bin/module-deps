#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_module-deps@2.0.6@module-deps/cmd.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_module-deps@2.0.6@module-deps/cmd.js" "$@"
  ret=$?
fi
exit $ret
