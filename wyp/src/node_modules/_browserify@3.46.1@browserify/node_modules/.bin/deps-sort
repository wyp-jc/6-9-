#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_deps-sort@0.1.2@deps-sort/bin/cmd.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_deps-sort@0.1.2@deps-sort/bin/cmd.js" "$@"
  ret=$?
fi
exit $ret
